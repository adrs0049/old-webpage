---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Andreas Buttenschoen</title>
	<link href="https://buttenschoen.ca/feed.xml" rel="self" />
	<link href="https://buttenschoen.ca/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://buttenschoen.ca/</id>
	<author>
		<name>Andreas Buttenschoen</name>
		<email>andreas@buttenschoen.ca</email>
	</author>

	{% for post in site.posts limit: 6 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="https://buttenschoen.ca{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>https://buttenschoen.ca{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}

</feed>
